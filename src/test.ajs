function angleBetweenClockHands(date) {
  // throw new Error('Not implemented');

  let hours = new Date(date).getUTCHours();
  let minutes = new Date(date).getMinutes();
  if (hours >= 12) {
    hours -= 12;
  }
  hours = 0.5 * (60 * hours + minutes);
  minutes = 6 * minutes;
  let result = Math.abs(hours - minutes);
  if (result > 180) {
    result -= 180;
  }
  return result * Math.PI / 180;
}

console.log(angleBetweenClockHands(Date.UTC(2016, 3, 5, 17, 24)));
console.log(angleBetweenClockHands(Date.UTC(2016, 3, 5, 5, 24)));
console.log(angleBetweenClockHands(Date.UTC(2016, 3, 5, 21, 0)));



